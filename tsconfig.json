{
  "extends": "@gravity-ui/tsconfig/tsconfig",
  "compilerOptions": {
    "noEmit": true,
    "target": "es5",
    "allowJs": false,
    "module": "ESNext",
    "moduleResolution": "Node10",
    "verbatimModuleSyntax": true,
    "noFallthroughCasesInSwitch": false
  },
  "include": ["**/*"],
  "exclude": ["build"]
}
